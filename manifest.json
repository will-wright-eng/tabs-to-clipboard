{
  "name": "Tabs to Clipboard",
  "version": "0.1.0",
  "description": "dev",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {},
  "commands": {
    "run-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Run \"foo\" on the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+U",
        "mac": "Command+Shift+U",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+U"
      }
    }
  },
  "content_scripts": [{
      "js": ["js/content.js"],
      "matches": ["*://*/*"]
  }]
}